<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="lab1.MainWindow"
        Title="lab1" Width="800" Height="600"
        Background="Turquoise">
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button Content="Просмотр" Click="ViewButton_ClickAsync"/>
        <Button Content="Редактирование" Click="EditButton_Click"/>
        <Button Content="Добавление" Click="AddButton_Click"/>
        <Button Content="Удаление" Click="DeleteButton_Click"/>

        <Button Content="Сформировать отчёт" Click="CreateDocsButton_Click"/>
    </StackPanel>
</Window>
