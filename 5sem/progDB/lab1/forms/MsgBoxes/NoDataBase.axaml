<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="600"
        x:Class="lab1.NoDataBase"
        Title="Вопрос" Width="400" Height="200"
        Background="Turquoise">
    
    <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="10">
        <TextBlock x:Name="MessageText" HorizontalAlignment="Center" Text=""/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="10">
            <Button Content="Да" Name="YesButton" Click="YesButton_Click"/>
            <Button Content="Нет" Name="NoButton" Click="NoButton_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
